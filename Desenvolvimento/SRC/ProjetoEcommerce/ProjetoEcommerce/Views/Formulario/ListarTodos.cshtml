@using ProjetoEcommerce.Dominio.Entidades.Seguranca
@model System.Collections.Generic.List<Formulario>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListarTodos</title>
</head>
<body>
    <div>
        <table>
            <thead>
                <tr>
                    <td>ID</td>
                    <td>Descrição</td>
                    <td>URL</td>
                    <td>Status</td>
                    <td>Usuário</td>
                    <td>Criado Em</td>
                    <td>Atualizado Em</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <td>@item.FormularioID</td>
                    <td>@item.Descricao</td>
                    <td>@item.URL</td>
                    <td>@item.Status</td>
                    <td>@item.Usuario</td>
                    <td>@item.CriadoEm</td>
                    <td>@item.AtualizadoEm</td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
