@model ProjetoEcommerce.Dominio.Entidades.Relatorio.CampoRelatorio

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Incluir</title>
</head>
<body>
    @using (Html.BeginForm("IncluirConfirm", "CampoRelatorio", FormMethod.Post))
    {
    <div style="border: 2px dotted black; padding: 45px">
        @Html.TextBoxFor(modelItem => Model.RelatorioID)
        @Html.TextBoxFor(modelItem => Model.CampoID)
        @Html.TextBoxFor(modelItem => Model.Usuario)
        @Html.TextBoxFor(modelItem => Model.Status)
        @Html.TextBoxFor(modelItem => Model.CriadoEm)
        @Html.TextBoxFor(modelItem => Model.AtualizadoEm)

    </div>
        <br />
        <br />
        <input type="submit" value="Confirmar" />
    }
</body>
</html>
